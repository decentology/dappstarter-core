{
  "name": "rust",
  "title": "Rust",
  "description": "",
  "imageUrl": "",
  "help": {
    "title": "",
    "url": ""
  },
  "blockchains": ["solana"],
  "interface": {
    "hidden": false,
    "enabled": true,
    "children": "none"
  },
  "targets": {
    "*": {
      "/package.project.json": {
        "processor": "copy-file"
      },
      "/~languages/rust/packages/dapplib/programs/src/lib.rs": {
        "processor": "merge-blocks",
        "path": "${categoryFolder}/${name}/rust/${name}.rs"
      },
      "/~languages/rust/packages/dapplib/src/scripts/layouts.js": {
        "processor": "merge-blocks",
        "path": "${categoryFolder}/${name}/rust/${name}-layout.js"
      },
      "/README.md": {
        "processor": "filter"
      },
      "/packages/client/README.md": {
        "processor": "filter"
      },
      "/packages/client/.babelrc": {
        "processor": "filter"
      },
      "/packages/dapplib/src/dapp-lib.js": {
        "processor": "merge-blocks",
        "path": "${categoryFolder}/${name}/cadence/${name}-lib.js"
      },
      "/~languages/rust/packages/dapplib/tests/[block-tests].js": {
        "processor": "file-blocks",
        "path": "${categoryFolder}/${name}/rust/${name}-tests.js",
        "sequence": false
      },
      "/packages/client/src/pages/harness/[block-pages].js": {
        "processor": "file-blocks",
        "path": "${categoryFolder}/${name}/cadence/${name}-page.js",
        "sequence": false
      },
      "/packages/client/src/[client-app]": {
        "processor": "copy-folder",
        "context": "framework"
      },
      "/packages/client/src/pages/harness/harness.js": {
        "processor": "replace-parameters"
      },
      "/packages/client/src/pages/harness/composer.js": {
        "processor": "replace-parameters"
      },
      "/packages/dapplib/src/[blockchain]": {
        "processor": "copy-folder",
        "context": "blockchain"
      },
      "/packages/dapplib/package.json": {
        "processor": "merge-json",
        "framework": {},
        "blockchain": {
          "*": {},
          "solana": {
            "scripts": {
              "clean": "rimraf -rf build && rimraf -rf dist",
              "deploy": "node src/scripts/deploy.js",
              "dev": "watch \"node src/scripts/deploy.js\" programs/src",
              "test": "npm run test"
            },
            "dependencies": {
            },
            "devDependencies": {
              "@solana/web3.js": "^1.11.0",
              "buffer-layout": "^1.2.0",
              "bs58": "^4.0.1",
              "chai": "^4.2.0",
              "mocha": "^8.1.3",
              "watch": "^1.0.2"
            }
          }
        },
        "block":  {
          "*": {},
          "onchain:core-token": {
            "devDependencies": {
              "@solana/spl-token": "^0.1.4"
            }
          }
        }
      },
      "/packages/client/package.json": {
        "processor": "merge-json",
        "blockchain": {},
        "framework": {
          "*": {},
          "react": {
            "devDependencies": {
              "@babel/preset-react": "^7.9.4",
              "react": "^16.13.0",
              "react-dom": "^16.13.0"
            }
          },
          "vanilla": {
          }
        }
      }
    }
  }
}
